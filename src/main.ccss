@import 'src/var.ccss';


#wrapper {
    @import flex;
    width: $width;

    &:hover {
        background: $bg;
    }

    &:active {
        background: #000;
    }

    a {
        @import flex;
        color: #333;

        &::before {
            content: '<';
            
        }
        &::after {
            content: '>';
        }
        &::before, &::after {
            color: #111;
        }
    }
}